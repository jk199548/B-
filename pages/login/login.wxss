/* pages/login/login.wxss */
page{
  background-color: #fafafa;
  display: flex;
  justify-content: center;
}
.logo{
  width: 100%;
  height: 150rpx;
  margin-top: 114rpx;
  display: flex;
  justify-content: center;
}
.logo image{
  width: 150rpx;
  height: 150rpx;
}
.name{
  width: 100%;
  margin-top: 20rpx;
  font-size: 36rpx;
  color: #FF9B09;
  display: flex;
  justify-content: center;
}
.dash{
  width: 625rpx;
  height: 2rpx;
  border-bottom: 1rpx dashed black;
  margin-top: 60rpx;
}
.notice{
  color: #0D0C11;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 25rpx;
}
.msg{
  display: flex;
  height: 30rpx;
  justify-content: flex-start;
  align-items: center;
  line-height: 30rpx;
  margin-top: 50rpx;
}
.msg view:nth-child(1){
  width: 11rpx;
  height: 11rpx;
  border-radius: 50%;
  background-color: #A7A7A7;
}
.msg view:nth-child(2){
  font-size: 30rpx;
  color: #828282;
  margin-left: 20rpx;
}
.btn{
  margin-top: 130rpx;
  width: 630rpx;
  border: none;
  background-color: #04BE01;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
}
